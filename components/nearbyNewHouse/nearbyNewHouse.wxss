.item-list{
    display: flex;
    margin-top: 26rpx;
    width: 90%;
    flex-direction: column;
    margin: auto;
}
.item-lsti-conent{
    border-bottom: 4rpx solid whitesmoke;
    display: flex;
    width: 90vw;
    padding: 15px 0;
    padding-left: 0px;
    margin: 10rpx 0;
}
.data02-img{
    position: relative;
    width: 216rpx;
    height: 164rpx;
    flex-shrink: 0;
    background-size: cover;
    border-radius: 4rpx;
    box-sizing: border-box;
}
.xs1{
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    right: 18px;
}
.xs4{
    background: red !important;
}
.xs2 , .xs4{
    width: 80rpx;
    height: 44rpx;
    text-align: center;
    line-height: 44rpx;
    background:#3fbaa6 ;
    letter-spacing: 2rpx;
    border: none;
    font-size: 24rpx;
    color: whitesmoke;
}
.data02-title{
    display: flex;
    flex-direction: column;
    width: 100vw;
    letter-spacing: 2rpx;
    padding-left: 30rpx;
    color: #05070ade;
    font-size: 24rpx;
    font-weight: 500;
}
.sw1{
    border: none !important;
}
.s3{
    margin: 16rpx 0;
    padding: 2rpx 0;
}
.s4 image{
    vertical-align: middle;
    margin: 0 6rpx;
    border-radius: 50%;
}
.s4 text{
    color: #101d37;
    font-size: 27rpx;
    padding: 10rpx 0;
    vertical-align: middle; 
}
.data02-title .s1{   
    width: 100%;
    font-size: 32rpx;
    font-weight: 700;
    color: #101d36;
    line-height: 42rpx;
    margin-bottom: 12rpx;
    overflow: hidden;
    word-wrap: break-word;
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    margin-top: -4rpx;
}
.hidden{
  display: none;
}
.data02-tupian{
  /* height: 160rpx; */
	position: relative;
}
.ysex01{
    width: 100%;
    background: #ff2929;
    color: whitesmoke;
    text-align: center;
    letter-spacing: 2px;
    position: absolute;
    z-index: 9;
    font-size: 26rpx;
		bottom: 0;
}
.q1,.q2{
    border-radius: 8rpx;
    background: #dff4f2;
    margin-right: 10rpx;
    padding: 4rpx 10rpx;
    color: #3fbaa6 !important;
    letter-spacing: 2rpx;
    height: 26rpx;
    text-align: center;
    line-height: 26rpx;
}