}
.list-foot{
  color: #cacaca;
   font-size: 12px !important;
   margin-top: 20px;
   margin-bottom:30px; 
}
.list-foot::after{
  content: '';
  display: block;
  width: 80px;
  height: 1px;
  background: #f5f5f5;
  position: absolute;
  top: 8px;
  left: -35px;
}
.list-foot::before{
  content: '';
  display: block;
  width: 80px;
  height: 1px;
  background: #f5f5f5;
  position: absolute;
  top: 8px;
  left: 215px;
}
