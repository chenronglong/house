}
.d1{
    margin-left: 20px;
    letter-spacing: 2px;
    font-size: 44rpx;
    color: #101d36;
    font-weight: 700;
}
.d2{
    margin-right: 20px;
    padding: 20rpx 10rpx;
    letter-spacing: 2rpx;
    color: #7a8190;
    font-size: 13px;
} 
.item-list{
    display: flex;
    margin-top: 13px;
    width: 90%;
    flex-direction: column;
    margin: auto;
}
/*取消边框样式*/
.sw1{
    border-bottom: none !important;
}
.item-lsti-conent{
    border-bottom: 2px solid whitesmoke;
    display: flex;
    width: 90vw;
    padding: 15px 0;
    padding-left: 0px;
    margin: 5px 0;
}
.data02-img{
    position: relative;
    width: 216rpx;
    height: 160rpx;
    flex-shrink: 0;
    background-size: cover;
    border-radius: 4rpx;
    box-sizing: border-box;
}
/**这里改整块租房描述*/
.data02-title{
    display: flex;
    flex-direction: column;
    width: 100vw;
    letter-spacing: 1px;
    padding-left: 30rpx;
    color: #05070ade;
    font-size: 12px;
}
.s3{
    padding: 3px 0;
}
.s4 image{
    vertical-align: middle;
    margin: 0 3px;
    border-radius: 50%;
}
/* 价格 头像 个人 */
.s4 text{
    color: #101d37;
    font-size: 27rpx;
    padding: 5px 0;
    vertical-align: middle; 
}
.data02-title .s1{   
    width: 100%;
    font-size: 32rpx;
    font-weight: 700;
    color: #101d36;
    line-height: 42rpx;
    margin-bottom: 12rpx;
    overflow: hidden;
    word-wrap: break-word;
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    margin-top: -4rpx; 
    /* letter-spacing: 1px;
    font-size: 28rpx;
