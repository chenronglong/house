.list-foot::after{
  content: '';
  display: block;
  width: 100px;
  height: 1px;
  background: #f5f5f5;
  position: absolute;
  top: 8px;
  left: -35px;
}
.list-foot::before{
  content: '';
  display: block;
  width: 100px;
  height: 1px;
  background: #f5f5f5;
  position: absolute;
  top: 8px;
  left: 200px;
}
