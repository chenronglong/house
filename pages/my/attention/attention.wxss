	background-color: #e2b81e;
	height: 35rpx;
	padding:0rpx 5rpx;
}
.title{
	font-weight: bold;
	margin-bottom: 10rpx;
}
.location{
	font-size: 25rpx;
}
.residence{
	font-size: 25rpx;
	margin-right: 20rpx;
}
.tags{
	background-color: #f3f3f3;
	font-size: 22rpx;
	color: #3ebecc;
	padding: 10rpx;
