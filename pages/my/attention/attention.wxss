.container{
	padding: 30rpx;
}
.title{
	font-weight: bold;
}
.time{
	font-size: 22rpx;
	color: #ccc;
	margin-top: 20rpx;
}
sup{
	 font-size: 10px;
	 vertical-align:text-top;
}
.item{
	display: flex;
	padding: 30rpx 0rpx;
	border-bottom: 1rpx solid #eeeeee;
}
.item image{
	width: 350rpx;
	height: 180rpx;
	margin-right: 20rpx;
}
.info{
	display: flex;
	width: 100%;
